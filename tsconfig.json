{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/component/home/CoreService.jsx",
    "src/app/component/home/HeroSectiom.jsx",
    "src/app/component/home/MultiSection.jsx",
    "src/shared/icon.js",
    "src/shared/SYContainer.jsx",
    "src/shared/SYButton.jsx",
    "src/shared/SubHeading.jsx",
    "src/component/Header.js"
  ],
  "exclude": ["node_modules"]
}
